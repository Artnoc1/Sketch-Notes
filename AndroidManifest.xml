<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.manichord.sketchnotes"
      android:versionCode="3"
      android:versionName="1.2">
    <application android:icon="@drawable/notebook_icon" android:label="@string/app_name">
        <activity android:name=".SKNotes"
                  android:label="@string/app_name"
                  android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity   android:name=".PagesList"
                    android:label="@string/pagelist_title">
        </activity>
        
        <activity android:name=".Prefs" android:label="@string/preferences_title">
        </activity>
        

    </application>
    <uses-sdk android:minSdkVersion="7" android:targetSdkVersion="12"/>

</manifest> 